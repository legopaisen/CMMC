@model CMMC.Models.Batch_Computation.BatchScheduleModel
         

<div>
  <div class="modal fade" id="divScheduleDetails" tabindex="-1" role="dialog" aria-labelledby="ScheduleDetails" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h5 class="modal-title">Batch Run Schedule Details</h5>
        </div>

        <div class="modal-body">
          <div class="">
            <div class="">
              <div class="well">
                <h5>Basic Details</h5>
                <ul id="listHere" class="list-group">
                  <li class="list-group-item"><strong>ID: </strong><span id="Request">@Model.ID.ToString().PadLeft(8, '0')</span></li>
                  <li class="list-group-item"><strong>Start Time: </strong><span id="Request">@Model.StartTime.ToString("MM/dd/yyyy hh:mm tt")</span></li>
                  <li class="list-group-item"><strong>Date to Process: </strong><span id="Request">@Model.ProcessDate.ToString("MM/dd/yyyy")</span></li>
                  <li class="list-group-item"><strong>Run Type: </strong><span id="Request">@Model.RunType.ToString()</span></li>
                  <li class="list-group-item"><strong>Status: </strong><span id="Request">@Model.Status.ToString()</span></li>
                  <li class="list-group-item"><strong>Remarks: </strong><span id="Request">@Model.Remarks</span></li>
                  <li class="list-group-item"><strong>Created By: </strong><span id="Request">@Model.CreatedBy</span></li>
                  <li class="list-group-item"><strong>Created On: </strong><span id="Request">@Model.CreatedOn.ToString("MM/dd/yyyy hh:mm tt")</span></li>
                </ul>
                <hr />
                <h5>Processes to run</h5>
                <ul class="list-group" id="detailsofRequest">
                  @foreach (var item in Model.Processes)
                  {
                    <li class="list-group-item"><span id="Request">@string.Format("{0} - {1}", item.ProcessCode, item.ProcessName)</span></li>
                  }
                  
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <div class="well">
      <h4>Requestor Details</h4>
      <ul id="listHere" class="list-group">
        <li class="list-group-item"><strong>Request: </strong><span id="Request">test</span></li>
      </ul>
      <hr />
      <h4>Details for Changes</h4>
      <ul class="list-group" id="detailsofRequest"></ul>
    </div>
  </div>
</div>

