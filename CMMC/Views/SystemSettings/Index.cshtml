@model CMMC.Models.SharedFunctions.Details
@{
 ViewBag.Title = "System Settings";
 Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">

 $(window).load(function () {
  var $EditFileAndBatch = $("#EditFileAndBatch"),
      $SaveFileandBatch = $("#SaveFileandBatch"),
      $CancelFileandBatch = $("#CancelFileandBatch"),
      $EditATMConnectionSettings = $("#EditATMSettings"),
      $SaveATMConnectionSettings = $("#SaveATMSettings"),
      $CancelATMConnectionSettings = $("#CancelATMSettings"),
      $EditFTPConnectionSettings = $("#EditFTPConSettings"),
      $SaveFTPConnectionSettings = $("#SaveFTPConSettings"),
      $CancelFTPConnectionSettings = $("#CancelFTPConSettings"),
      $EditADB = $("#EditADB"),
      $SaveADB = $("#SaveADB"),
      $CancelADB = $("#CancelADB"),
      $EditNotificationSetting = $("#EditNotificationSettings"),
      $SaveNotificationSetting = $("#SaveNotificationSettings"),
      $CancelNotificationSetting = $("#CancelNotificationSettings"),
      $EditConn = $("#EditConnection"),
      $SaveConnection = $("#SaveSettingConnection"),
      $CancelConnection = $("#CancelConnection"),
        $SaveODS = $("#SaveODS"),
         $SaveGBPMFTP = $("#SaveGBPMFTP")


  if (localStorage.getItem("Status") == "successfilebatch") {
   Command: toastr["success"]("Success", "Successful update for file and batch program settings.")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "errorfilebatch") {
   Command: toastr["error"]("Error", "Error update for file and batch program settings.")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "successatmsettings") {
   Command: toastr["success"]("Success", "Successful update for ATM System Database Connection Settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "erroratmsettings") {
   Command: toastr["error"]("Error", "Error updating for ATM System Database Connection Settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "successftp") {
   Command: toastr["success"]("Success", "Successful update for FTP Connection Settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "errorftp") {
   Command: toastr["error"]("Error", "Error updating for FTP Connection Settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "successnotif") {
   Command: toastr["success"]("Success", "Successful update for Notification Settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "errornotif") {
   Command: toastr["error"]("Error", "Error updating for Notification Settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "successset") {
   Command: toastr["success"]("Success", "Successful update for SQL Connection settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "errorset") {
   Command: toastr["error"]("Error", "Error updating SQL Connection settings")
   localStorage.clear();
  }
  if (localStorage.getItem("Status") == "successadb") {
      Command: toastr["success"]("Success", "Successful update for  ADB Transfer file details")
      localStorage.clear();
  }
  if (localStorage.getItem("Status") == "erroradb") {
      Command: toastr["error"]("Error", "Error updating ADB Transfer file details")
      localStorage.clear();
    }
    if (localStorage.getItem("Status") == "successods") {
        Command: toastr["success"]("Success", "Successful update for ODS Connection settings")
        localStorage.clear();
    }
    if (localStorage.getItem("Status") == "errorods") {
        Command: toastr["error"]("Error", "Error updating ODS Connection settings")
        localStorage.clear();
     }
     if (localStorage.getItem("Status") == "successmftp") {
         Command: toastr["success"]("Success", "Successful update for GBP MFTP Connection settings")
         localStorage.clear();
     }
     if (localStorage.getItem("Status") == "errormftp") {
         Command: toastr["error"]("Error", "Error updating GBP MFTP Connection settings")
         localStorage.clear();
     }

  //Save File and Batch
  $SaveFileandBatch.click(function () {
   var model =  {
    FileName: $("#FileName").val(),
    FileLoadPath: $("#FileLoadPath").val(),
    FMTFilesPath: $("#FMTFilesPath").val(),
    InvvPath: $("#InvvPath").val(),
    AcctPath: $("#AcctPath").val(),
    CustPath: $("#CustPath").val(),
    ADBCasaPath: $("#ADBCasaPath").val(),
    BackUp: $("#BackUp").val(),
    BackUpPath: $("#BackUpPath").val(),
    BulkInsert: $("#BulkInsert").val(),
    DumpinInvvPath: $("#DumpinInvvPath").val(),
    ProgramName: $("#ProgramName").val(),
    FNSAdbFile: $("#FNSAdbFile").val(),
    ChargingFilePath: $("#ChargingFilePath").val(),
    UpdateAcct: $("#UpdateAcct").val(),
    AcctOnePath: $("#AcctOnePath").val(),
    UpdateCust: $("#UpdateCust").val(),
    CustOnePath: $("#CustOnePath").val(),
    MaintenanceSubject: $("#MaintenanceSubject").val(),
    MaintenanceRecipient: $("#MaintenanceRecipient").val()
      }
         $.ajax({
          data: JSON.stringify({ fileandBatch: model }),
             url: '@Url.Action("SaveFileAndBatch", "SystemSettings")',
             type: 'POST',
             contentType: 'application/json; charset=utf8',
    success: function (data) {
     location.reload();
     localStorage.setItem("Status", "successfilebatch")
    },
    error: function (e) {
        alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("Status", "errorfilebatch")
    }
   });
   $("#EditFileAndBatchModal").modal('hide');
  });

 //Save Atm Settings
  $SaveATMConnectionSettings.click(function () {
      var model = {
       PowercardServer: $("#PowercardServer").val(),
       PowercardPort: $("#PowercardPort").val(),
       PowercardServiceName: $("#PowercardServiceName").val(),
       PowercardUserID: $("#PowercardUserID").val(),
       POwercardPassword: $("#PowercardPassword").val()
      }
   $.ajax({
    data: JSON.stringify({ atmcon: model }),
       url: '@Url.Action("SaveATMDatabaseSettings", "SystemSettings")',
       type: 'POST',
       contentType: 'application/json; charset=utf8',
    success: function () {
     location.reload();
     localStorage.setItem("Status", "successatmsettings")
    },
    error: function (e) {
        alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("Status", "erroratmsettings")
    }
   });
   $("#EditATMModal").modal('hide');
  });

  //Save ADB
  $SaveADB.click(function () {
   var model = {
    ADBDatabase: $("#DatabaseADB").val(),
    DBID: $("#DatabaseID").val(),
    JobName: $("#ADBJobName").val(),
    ADBPassword: $("#PasswordADB").val(),
    ADBServer: $("#ServerADB").val()
   }
   $.ajax({
    data: JSON.stringify({ adbfiles: model }),
    url: '@Url.Action("SaveADB", "SystemSettings")',
    type: 'POST',
    contentType: 'application/json; charset=utf8',
    success: function () {
     location.reload();
     localStorage.setItem("Status", "successadb")
    },
    error: function () {
     alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("status", "erroradb")
    }
   });
   $("#EditADBModal").modal('hide');
  });

  //Save FTP Connection Settings
  $SaveFTPConnectionSettings.click(function () {
   var model = {
    FTPIPAddress: $("#FTPIPAddress").val(),
    FTPTimeout: $("#FTPTimeout").val(),
    FTPDestination: $("#FTPDestination").val(),
    FTPUserID: $("#FTPUserID").val(),
    FTPPassword: $("#FTPPassword").val(),
    FTPDestinationFileName: $("#FTPDestinationFileName").val()
   }
   $.ajax({
    data: JSON.stringify({ ftp: model }),
    url: '@Url.Action("SaveFTPConnectionSetting", "SystemSettings")',
    type: 'POST',
    contentType: 'application/json; charset=utf8',
    success: function () {
     location.reload();
     localStorage.setItem("Status", "successftp")
    },
    error: function () {
     alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("status", "successftp")
    }
   });
   $("#EditFTPModal").modal('hide');
  });

  $SaveNotificationSetting.click(function () {
   var model = {
    SMPTPServer: $("#SMPTPServer").val(),
    Sender: $("#Sender").val(),
    SubjectError: $("#SubjectError").val(),
    SubjectSuccess: $("#SubjectSuccess").val(),
    BodyError: $("#BodyError").val(),
    BodySuccess: $("#BodySuccess").val(),
    LogFilePath: $("#LogFilePath").val(),
    Email: $("#Email").val(),
    AccountEmailRecipient: $("#AccountEmail").val(),
    AccountEmailSubject: $("#AccountSubject").val()
   }
   $.ajax({
    data: JSON.stringify({ notif: model }),
    url: '@Url.Action("SaveNotificationSettings", "SystemSettings")',
    type: 'POST',
    contentType: 'application/json; charset=utf8',
    success: function () {
     location.reload();
     localStorage.setItem("Status", "successnotif")
    },
    error: function () {
     alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("status", "errornotif")
    }
   });
   $("#EditNotificationModal").modal('hide');
  });

 //Saveconnection
  $SaveConnection.click(function () {
      var model = {
          SQLServer: $("#Server").val(),
          SQLDatabase: $("#Database").val(),
          SQLUserID: $("#DBID").val(),
          SQLPassword: $("#Password").val(),
          SecurityKey: $("#SecurityKey").val(),
          UserID: $("#UserID").val(),
          Schedule: $("#Schedule").val(),
          Fee: $("#Fee").val(),
          INVTYPE_MOTHER: $("#INVTYPE_MOTHER").val(),
          INVTYPE_CHILD: $("#INVTYPE_CHILD").val()
      }
   $.ajax({
       data: JSON.stringify({ sett: model }),
    url: '@Url.Action("SaveConnectionSetting", "SystemSettings")',//'/SystemSettings/SaveConnectionSetting',
       type: 'POST',
       contentType: 'application/json; charset=utf8',
    success: function (data) {
     location.reload();
     localStorage.setItem("Status", "successset")
    },
    error: function (e) {
        alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("Status", "errorset")
    }
   });

   $("#EditConnectionModal").modal('hide');
  });

     $SaveODS.click(function () {
      var model = {
          ODSServerIP: $("#ODSServerIP").val(),
          ODSUserID: $("#ODSUserID").val(),
          ODSPassword: $("#ODSPassword").val(),
          ODSPort: $("#ODSPort").val(),
          ODSServiceName: $("#ODSServiceName").val(),
          ODSDatabase: $("#ODSDbID").val()
      }
   $.ajax({
       data: JSON.stringify({ sett: model }),
    url: '@Url.Action("SaveODSConnectionSettings", "SystemSettings")',
       type: 'POST',
       contentType: 'application/json; charset=utf8',
    success: function (data) {
     location.reload();
     localStorage.setItem("Status", "successods")
    },
    error: function (e) {
        alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("Status", "errorods")
    }
   });

         $("#EditODSConnectionModal").modal('hide');
     });

     $SaveGBPMFTP.click(function () {
      var model = {
          GBPMFTPIP: $("#GBPMFTPIP").val(),
          GBPMFTPPassword: $("#GBPMFTPPassword").val(),
          GBPMFTPPath: $("#GBPMFTPPath").val(),
          GBPMFTPResponsePath: $("#GBPMFTPResponsePath").val(),
          GBPMFTPPort: $("#GBPMFTPPort").val(),
          GBPMFTPUser: $("#GBPMFTPUser").val()
      }
   $.ajax({
       data: JSON.stringify({ sett: model }),
    url: '@Url.Action("SaveGBPMFTPConnectionSettings", "SystemSettings")',
       type: 'POST',
       contentType: 'application/json; charset=utf8',
    success: function (data) {
     location.reload();
     localStorage.setItem("Status", "successmftp")
    },
    error: function (e) {
        alert(JSON.stringify(e));
     location.reload();
     console.log(JSON.stringify(e));
     localStorage.setItem("Status", "errormftp")
    }
   });

    $("#EditGBPMFTPConnectionModal").modal('hide');
  });

 });//END OF WINDOW LOAD
</script>

<div class="container" id="sysDetails">
    <div class="row">
        <h2 class="page-header">
            System Settings
        </h2>
        <div class="row">
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        File and Batch Program Settings
                        <small> -- Used for file directory setup</small>
                        <span class="pull-right">
                            <button id="EditFileAndBatch" data-toggle="modal" data-target="#EditFileAndBatchModal" class="btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>File Name: </strong> @Model.FileAndBatch.FileName</li>
                        <li class="list-group-item"><strong>File Load Path: </strong> @Model.FileAndBatch.FileLoadPath</li>
                        <li class="list-group-item"><strong>FMT Files Path: </strong> @Model.FileAndBatch.FMTFilesPath</li>
                        <li class="list-group-item"><strong>INVV Path: </strong> @Model.FileAndBatch.InvvPath</li>
                        <li class="list-group-item"><strong>Account Path: </strong> @Model.FileAndBatch.AcctPath</li>
                        <li class="list-group-item"><strong>Customer Path: </strong> @Model.FileAndBatch.CustPath</li>
                        <li class="list-group-item"><strong>ADB CASA Path:</strong> @Model.FileAndBatch.ADBCasaPath</li>
                        <li class="list-group-item"><strong>Back Up: </strong> @Model.FileAndBatch.BackUp</li>
                        <li class="list-group-item"><strong>Back Up Path: </strong> @Model.FileAndBatch.BackUpPath</li>
                        <li class="list-group-item"><strong>Bulk Insert Folder: </strong> @Model.FileAndBatch.BulkInsert</li>
                        <li class="list-group-item"><strong>Dump INVV Path: </strong> @Model.FileAndBatch.DumpinInvvPath</li>
                        <li class="list-group-item"><strong>Program Name: </strong> @Model.FileAndBatch.ProgramName</li>
                        <li class="list-group-item"><strong>FNS ADB File: </strong> @Model.FileAndBatch.FNSAdbFile</li>
                        <li class="list-group-item"><strong>Charging File Path:</strong> @Model.FileAndBatch.ChargingFilePath</li>
                        <li class="list-group-item"><strong>Update Account: </strong> @Model.FileAndBatch.UpdateAcct</li>
                        <li class="list-group-item"><strong>Account One Path: </strong> @Model.FileAndBatch.AcctOnePath</li>
                        <li class="list-group-item"><strong>Update Customer: </strong> @Model.FileAndBatch.UpdateCust</li>
                        <li class="list-group-item"><strong>Customer One Path:</strong> @Model.FileAndBatch.CustOnePath</li>
                        <li class="list-group-item"><strong>Maintenance Email Subject: </strong> @Model.FileAndBatch.MaintenanceSubject</li>
                        <li class="list-group-item"><strong>Maintenance Email Recipients: </strong> @Model.FileAndBatch.MaintenanceRecipient</li>
                    </ul>
                    <hr />
                </div>
            </div>
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        ATM System (Powercard) Database Connection Settings
                        <small> -- For updating of CMS codes in Powercard Database</small>
                        <span class="pull-right">
                            <button id="EditATMSettings" data-toggle="modal" data-target="#EditATMModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>Powercard Server: </strong> @Model.ATMSystemConnection.PowercardServer</li>
                        <li class="list-group-item"><strong>Powercard Port: </strong> @Model.ATMSystemConnection.PowercardPort</li>
                        <li class="list-group-item"><strong>Powercard Service Name: </strong> @Model.ATMSystemConnection.PowercardServiceName</li>
                        <li class="list-group-item"><strong>Powercard User ID: </strong> @Model.ATMSystemConnection.PowercardUserID</li>
                    </ul>
                    <hr />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        FTP Connection Settings
                        <small> -- Used for file transfer of CMMC Charging File to BANCS</small>
                        <span class="pull-right">
                            <button id="EditFTPConSettings" data-toggle="modal" data-target="#EditFTPModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>FTP IP ADDRESS: </strong> @Model.FTPConSettings.FTPIPAddress</li>
                        <li class="list-group-item"><strong>FTP Timeout: </strong> @Model.FTPConSettings.FTPTimeout</li>
                        <li class="list-group-item"><strong>FTP Destination: </strong> @Model.FTPConSettings.FTPDestination</li>
                        <li class="list-group-item"><strong>FTP User ID: </strong> @Model.FTPConSettings.FTPUserID</li>
                        @*<li class="list-group-item"><strong>FTP Password:</strong> @Model.FTPConSettings.FTPPassword</li>*@
                        <li class="list-group-item"><strong>FTP Destination File Name:</strong> @Model.FTPConSettings.FTPDestinationFileName</li>
                    </ul>
                    <hr />
                </div>
            </div>
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        Notification Settings
                        <small> -- Email Notification Parameters</small>
                        <span class="pull-right">
                            <button id="EditNotificationSettings" data-toggle="modal" data-target="#EditNotificationModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>SMTP Server: </strong> @Model.NotificationSettings.SMPTPServer</li>
                        <li class="list-group-item"><strong>Sender: </strong> @Model.NotificationSettings.Sender</li>
                        <li class="list-group-item"><strong>Subject Error: </strong> @Model.NotificationSettings.SubjectError</li>
                        <li class="list-group-item"><strong>Subject Success: </strong> @Model.NotificationSettings.SubjectSuccess</li>
                        <li class="list-group-item"><strong>Body Error:</strong> @Model.NotificationSettings.BodyError</li>
                        <li class="list-group-item"><strong>Body Success:</strong> @Model.NotificationSettings.BodySuccess</li>
                        <li class="list-group-item"><strong>Log File Path:</strong> @Model.NotificationSettings.LogFilePath</li>
                        <li class="list-group-item"><strong>E- Mail:</strong> @Model.NotificationSettings.Email</li>
                        <li class="list-group-item"><strong>Account Email Recipients: </strong> @Model.NotificationSettings.AccountEmailRecipient</li>
                        <li class="list-group-item"><strong>Account Email Subject: </strong> @Model.NotificationSettings.AccountEmailSubject</li>
                    </ul>
                    <hr />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        General System Settings
                        <span class="pull-right">
                            <button id="EditConnection" data-toggle="modal" data-target="#EditConnectionModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>Server Name: </strong> @Model.Settings.SQLServer</li>
                        <li class="list-group-item"><strong>Database: </strong> @Model.Settings.SQLDatabase</li>
                        <li class="list-group-item"><strong>Database ID: </strong> @Model.Settings.SQLUserID</li>
                        @*<li class="list-group-item"><strong>Password: </strong>@Model.Settings.SQLPassword</li>*@
                        <li class="list-group-item"><strong>Security Key: </strong> @Model.Settings.SecurityKey</li>
                        <li class="list-group-item"><strong>User ID: </strong> @Model.Settings.UserID</li>
                        <li class="list-group-item"><strong>Schedule Day: </strong> @Model.Settings.Schedule</li>
                        <li class="list-group-item"><strong>Withdrawal Fee: </strong> @Model.Settings.Fee</li>
                        <li class="list-group-item"><strong>Investment Mother Account: </strong> @Model.Settings.INVTYPE_MOTHER</li>
                        <li class="list-group-item"><strong>Investment Child Account: </strong> @Model.Settings.INVTYPE_CHILD</li>
                    </ul>
                    <hr />
                </div>
            </div>
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        ADB DTS Connection Settings
                        <small> -- Used for FNS_ADB bulk loading on month-end processing</small>
                        <span class="pull-right">
                            <button id="EditADB" data-toggle="modal" data-target="#EditADBModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>Server: </strong> @Model.ADBTransferFile.ADBServer</li>
                        <li class="list-group-item"><strong>ADB Database: </strong> @Model.ADBTransferFile.ADBDatabase</li>
                        <li class="list-group-item"><strong>Database ID: </strong> @Model.ADBTransferFile.DBID</li>
                        @*<li class="list-group-item"><strong>Password: </strong> @Model.ADBTransferFile.ADBPassword</li>*@
                        <li class="list-group-item"><strong>ADB Jobname: </strong> @Model.ADBTransferFile.JobName</li>
                    </ul>
                    <hr />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        ODS Connection Settings
                        <span class="pull-right">
                            <button id="EditODSConnection" data-toggle="modal" data-target="#EditODSConnectionModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>Server: </strong> @Model.ODSConnectionSetting.ODSServerIP</li>
                        <li class="list-group-item"><strong>Database: </strong> @Model.ODSConnectionSetting.ODSDatabase</li>
                        <li class="list-group-item"><strong>User ID: </strong> @Model.ODSConnectionSetting.ODSUserID</li>
                        <li class="list-group-item"><strong>Password: </strong>@Model.ODSConnectionSetting.ODSPassword</li>
                        <li class="list-group-item"><strong>Port: </strong>@Model.ODSConnectionSetting.ODSPort</li>
                        <li class="list-group-item"><strong>Service Name: </strong>@Model.ODSConnectionSetting.ODSServiceName</li>
                    </ul>
                    <hr />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="well">
                    <h3>
                        GBP MFTP Connection Settings
                        <span class="pull-right">
                            <button id="EditGBPMFTPConnection" data-toggle="modal" data-target="#EditGBPMFTPConnectionModal" class=" btn btn-success btn-sm glyphicon glyphicon-pencil"></button>
                        </span>
                    </h3>
                    <hr />
                    <ul class="list-group">
                        <li class="list-group-item"><strong>Server: </strong> @Model.GBPMFTPConnectionSetting.GBPMFTPIP</li>
                        <li class="list-group-item"><strong>Path: </strong> @Model.GBPMFTPConnectionSetting.GBPMFTPPath</li>
                        <li class="list-group-item"><strong>Response Path: </strong> @Model.GBPMFTPConnectionSetting.GBPMFTPResponsePath</li>
                        <li class="list-group-item"><strong>User: </strong> @Model.GBPMFTPConnectionSetting.GBPMFTPUser</li>
                        <li class="list-group-item"><strong>Password: </strong>@Model.GBPMFTPConnectionSetting.GBPMFTPPassword</li>
                        <li class="list-group-item"><strong>Port: </strong>@Model.GBPMFTPConnectionSetting.GBPMFTPPort</li>
                    </ul>
                    <hr />
                </div>
            </div>
        </div>
    </div>
</div>
