<div id="EditODSConnectionModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background-color: teal; color: white;">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Edit ODS Connection Settings</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <div class="row" id="sourcefile">
                        <div class="col-sm-12">
                            <div class="panel panel-default">
                                <div class="panel-body">
                                    <div class="form-group">
                                        <div class="input-group">
                                            <span class="input-group-addon">Server:</span>
                                            <input id="ODSServerIP" class="form-control" type="text" autocomplete="off" value="@Model.ODSConnectionSetting.ODSServerIP" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="input-group">
                                            <span class="input-group-addon">Database:</span>
                                            <input id="ODSDbID" class="form-control" type="text" value="@Model.ODSConnectionSetting.ODSDatabase" />
                                        </div>
                                    </div>
